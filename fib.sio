func fib(x) {
    if x < 0 {
        print x;
        print "Not a valid number!"
    } 
    else {
        if x is 1 {
            return 0
        } 
        else {
            if x is 2 {
                return 1
            } 
            else {
                return fib(x-1)+fib(x-2)
            }
        }
    }
};

print fib(20)
